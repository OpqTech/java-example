	agent { label 'slave}
	stages{
        stage('Build stage') {
            steps {
                echo 'This is a build stage'
				
			}
		}
        stage('Push stage') {
            steps {
                echo 'This is push stage'
                
			}
		}
        stage('Deploy stage') {
            steps {
                echo 'This is deploy stage'
          
			}
		}
	}
}	
